    
    //alphabets printing pattern
    
  #include <stdio.h>
int main()
    {
        int n,i,j;
        char c='A',s='a';
        scanf("%d",&n);
        for(i=1;i<=n;i++)
        {
            for(j=1;j<=i;j++)
            {
                printf("%c%c\t",c,s);
                c++,s++;
            }
        
            printf("\n");
        }
      return 0;
   }  
    
    OUTPUT 1:
    
    Aa  
    Bb   Cc
    Dd   Ee   Ff
    Gg   Hh   Ii   Jj
    Kk   Ll   Mm   Nn   Oo
    
    
     OUTPUT 2:
     
     Aa  
     Bb   Cc
     Dd   Ee   Ff
