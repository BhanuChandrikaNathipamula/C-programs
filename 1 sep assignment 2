
#include <stdio.h>
void  main() {
   int s,result;
   printf("Enter a number");
   scanf("%d",&s);
   result=fact(s);
   printf("Factorial is %d",result);
}
int fact(int x)
{
    int i,fact=1;
    for(i=1;i<=x;i++)
    {
        fact=fact*i;
    }
    return(fact);
}

OUTPUT:
Enter a number
5
Factorial is 120
    
